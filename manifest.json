{
  "name": "Gmail Contrast New Look",
  "version": "0.1.0",
  "manifest_version": 2,

  "description": "Make default Gmail New Look (2012) more readable by tweaking contrast, margins and borders.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/mail/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "*://mail.google.com/mail/*"
  ],
  "options_page": "options.html"
}
